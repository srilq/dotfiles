[user]
  email = git@srilq.email
  name = Stef J
[hub]
  protocol = https
[alias]
  co = checkout
  st = status -sb
  wtf = !$ZSH/bin/git-wtf
[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
[difftool]
  prompt = false
[help]
  autocorrect = 5
[push]
  default = current
[credential]
  helper = osxkeychain
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
